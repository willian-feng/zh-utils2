{
  "compilerOptions": {
    "baseUrl": ".",
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./lib",
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["es/**/*.ts"]
}
